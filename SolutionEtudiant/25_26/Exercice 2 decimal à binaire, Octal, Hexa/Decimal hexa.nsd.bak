<root xmlns:nsd="https://structorizer.fisch.lu" version="3.30-10" preRepeat="until " postFor="to" preReturn="return" postForIn="in" preWhile="while " output="OUTPUT" input="INPUT" preFor="for" preExit="exit" preLeave="leave" ignoreCase="true" preThrow="throw" preForIn="foreach" stepFor="by" author="timDoleyres" created="2025-08-28" changedby="timDoleyres" changed="2025-09-02" origin="Structorizer 3.30-10" text="&#34;Timéo Doleyres&#34;,&#34;Steeve Pouly&#34;,&#34;Christophe Allenbach&#34;,&#34;&#34;,&#34;Decimal en Hexa&#34;,&#34;02.09.2025&#34;" comment="&#34;&#34;" color="ffffff" type="sub" style="nice">
	<children>
		<instruction text="&#34;&#34;&#34;ValDec&#34;&#34; variable 8 bits non signé&#34;,&#34;&#34;&#34;ValHex&#34;&#34; variable virgule flotante&#34;,&#34;&#34;&#34;ValRec&#34;&#34; variable 8 bits non signé&#34;" comment="&#34;&#34;" color="ffff80" rotated="0" disabled="0"></instruction>
		<instruction text="&#34;Recuperer la valeur du clavier dans &#34;&#34;ValDec&#34;&#34;&#34;" comment="" color="ffff80" rotated="0" disabled="0"></instruction>
		<for text="&#34;for i &#60;- 0 to 7&#34;" comment="" counterVar="i" startValue="0" endValue="7" stepConst="1" style="COUNTER" color="ff8000" disabled="0">
			<qFor>
				<instruction text="&#34;division par 16 de &#34;&#34;ValDec&#34;&#34; saugarder dans &#34;&#34;ValHex&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
				<instruction text="&#34;division par 16 de &#34;&#34;ValDec&#34;&#34; saugarder dans &#34;&#34;ValRec&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
				<instruction text="&#34;soustraire &#34;&#34;ValRec&#34;&#34; à &#34;&#34;ValHex&#34;&#34;, saugarder dans &#34;&#34;ValHex&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
				<instruction text="&#34;Multiplier par 16 &#34;&#34;ValHex&#34;&#34;, saugarder dans &#34;&#34;ValHex&#34;&#34;&#34;" comment="&#34;&#34;" color="ffffff" rotated="0" disabled="0"></instruction>
				<instruction text="&#34;saugarder la valeur de &#34;&#34;ValRec&#34;&#34; dans &#34;&#34;ValDec&#34;&#34;&#34;" comment="" color="ffffff" rotated="0" disabled="0"></instruction>
				<alternative text="&#34;Si(&#34;&#34;ValHex&#34;&#34; est plus petit que 10)&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
					<qTrue>
						<instruction text="&#34;afficher &#34;&#34;ValDec&#34;&#34; : 16 = &#34;&#34;ValRec&#34;&#34; R = &#34;&#34;ValHex&#34;&#34;&#34;" comment="" color="80ff80" rotated="0" disabled="0"></instruction>
					</qTrue>
					<qFalse>
						<instruction text="&#34;afficher avec la table ascii &#34;&#34;ValDec&#34;&#34; : 16 = &#34;&#34;ValRec&#34;&#34; R = (&#34;&#34;ValHex&#34;&#34; - 10) + 65&#34;" comment="&#34;&#34;" color="80ff80" rotated="0" disabled="0"></instruction>
					</qFalse>
				</alternative>
				<instruction text="&#34;Retour a la ligne&#34;" comment="" color="80ff80" rotated="0" disabled="0"></instruction>
			</qFor>
		</for>
	</children>
</root>